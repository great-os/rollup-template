{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module",
    "ecmaFeatures": {
    }
  },
  "extends": ["airbnb-base", "plugin:jest/recommended"],
  "rules": {
    "semi": ["error", "never"],
    "comma-dangle": ["error", "always-multiline"],
    "no-console": 0
  },
  "plugins": ["jest"],
  "env": {
    "jest/globals": true
  },
  "settings": {
    "import/resolver": {
      "jest": {}
    }
  }
}
